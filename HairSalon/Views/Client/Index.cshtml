<h1>Clients</h1>
<p><a href='/items/new'>Add a New Client<a/></p>
<p><a href='/'>Back to Home<a/></p>
<table>
  <tr>
    <th>Name</th>
    <th>Update</th>
    <th>Delete</th>
  </tr>
  @foreach(var item in Model)
  {
  <tr>
    <td>@item.GetClientName()</td>
    <td><a href="/items/@item.GetClientId()/update">Edit</a></td>
    <td><a href="/items/@item.GetClientId()/delete">Delete</a></td>
  </tr>
  }
</table>
